package dto;

public class EmployeeDTO {
	private String eno;
	private String name;
	private String department;
	private int poistion;
	private int salary;
	
	
	public EmployeeDTO(String eno, String name, String department, int poistion) {
		super();
		this.eno = eno;
		this.name = name;
		this.department = department;
		this.poistion = poistion;
	}


	public String getEno() {
		return eno;
	}


	public void setEno(String eno) {
		this.eno = eno;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getDepartment() {
		return department;
	}


	public void setDepartment(String department) {
		this.department = department;
	}


	public int getPoistion() {
		return poistion;
	}


	public void setPoistion(int poistion) {
		this.poistion = poistion;
	}





	@Override
	public String toString() {
		return "EmployeeVO [eno=" + eno + ", name=" + name + ", department=" + department + ", poistion=" + poistion
				+ "]";
	}
	
	
}
