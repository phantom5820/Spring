select * from EMPLOYEE;
select * from EMPLOYEE_SALARY;
select * from POSITION_LIST;

select rownum, eno, name, department, position_name, salary from(select e.eno, e.name, e.department, p.position_name, s.salary from EMPLOYEE e, POSITION_LIST p, EMPLOYEE_SALARY s where e.eno = s.eno and e.position = p.position = p.position_no order by s.salary asc)where rownum <= 5